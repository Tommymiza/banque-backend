{
    "version": 2,
    "buildCommand": "node ace build --production --ignore-ts-errors",
    "outputDirectory": "build",
    "installCommand": "cd build && npm ci -- production",
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }